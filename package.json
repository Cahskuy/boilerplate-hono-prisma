{
  "name": "boilerplate-hono-prisma",
  "scripts": {
    "dev": "bun --hot src/server.ts",
    "build": "bun build --target bun src/server.ts --outdir dist",
    "start": "bun dist/server.js",
    "prisma:gen": "bunx prisma generate",
    "prisma:migrate": "bunx prisma migrate dev"
  },
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.9.1",
    "dotenv": "^17.2.3",
    "prisma": "^6.18.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "croner": "^9.1.0",
    "hono": "^4.10.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "zod": "^4.1.12"
  }
}
